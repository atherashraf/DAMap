{
  "name": "damap",
  "version": "0.1.27",
  "private": false,
  "author": "Ather Ashraf",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "description": "Digital Arz Maps",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "npm run clean && npm run build:esm && npm run copy-files",
    "clean": "rm -rf ./dist",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.jpg src/**/*.png dist/esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly-old": "npm test && npm run prettier && npm run lint --no-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atherashraf/DAMap.git "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "digitalarz-map",
    "damap"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@mui/x-date-pickers": "^6.15.0",
    "@mui/x-tree-view": "^6.0.0-alpha.4",
    "auto-bind": "^5.0.1",
    "dayjs": "^1.11.10",
    "geostyler-legend": "^4.0.0",
    "geostyler-openlayers-parser": "^4.2.1",
    "geostyler-sld-parser": "^5.1.0",
    "highcharts": "^11.1.0",
    "highcharts-react-official": "^3.2.1",
    "jqwidgets-scripts": "^17.0.1",
    "ol": "^8.1.0",
    "ol-ext": "^4.0.11",
    "pako": "^2.1.0",
    "plotly.js": "^2.28.0",
    "react-color": "^2.19.3",
    "react-pivottable": "^0.11.0",
    "react-plotlyjs": "^0.4.4",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"

  },
  "devDependencies": {
    "@siedlerchr/types-ol-ext": "^3.2.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/geojson": "^1.0.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.54",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^2.3.0",
    "@types/pako": "^2.0.1",
    "@types/react": "^18.2.23",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.8",
    "@types/react-pivottable": "^0.11.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^28.0.4",
    "typescript": "^4.9.5"
  }
}
